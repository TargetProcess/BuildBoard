{
  "branches": {
    "release": "^(?:origin/)?release/(.*)$",
    "feature": "^(?:origin/)?feature/(.*)$",
    "hotfix": "^(?:origin/)?hotfix/(.*)$",
    "vs": "^(?:origin/)?vs/(.*)$",
    "develop": "^(?:origin/)?develop$",
    "other":"^(?:origin/)?(?!(develop$)|(release/)|(feature/)|(vs/)|(hotfix/))(.*)$"
  },
  "build": {
    "cycles": [
      {
        "name": "Package only",
        "branches": [
          "all"
        ],
        "parameters": {
          "isFull": false,
          "includeUnstable": false,
          "includeDb": false,
          "karmaTests": [],
          "includeComet": false,
          "includeSlice": false,
          "buildFullPackage": true,
          "casperTests": [],
          "unitTests": [],
          "pythonFuncTests": [],
          "funcTests": [],
          "includePerfTests" : false
        }
      },
      {
        "name": "Release",
        "branches": [
          "all"
        ],
        "parameters": {
          "isFull": true,
          "includeUnstable": false,
          "includeDb": true,
          "karmaTests": [
            "tests",
            "tpTests"
          ],
          "includeComet": true,
          "includeSlice": true,
          "buildFullPackage": true,
          "casperTests": [
            "general",
            "qaarea",
            "stats",
            "timeline"
          ],
          "unitTests": ["All"],
          "pythonFuncTests": ["All"],
          "funcTests": ["All"],
          "includePerfTests" : true
        }
      },
      {
        "name": "Debug",
        "branches": [
          "feature",
          "vs",
          "develop",
          "other"
        ],
        "parameters": {
          "isFull": false,
          "includeUnstable": false,
          "includeDb": false,
          "karmaTests": [
            "tests",
            "tpTests"
          ],
          "includeComet": false,
          "includeSlice": false,
          "includePerfTests" : false,
          "buildFullPackage": false,
          "casperTests": [
            "general",
